<import src="../../template/na_tabBar_template.wxml" />
<template is="na_tabBar_template" data="{{na_tabbar}}" />

<view class="page">
  
  <view class="page__bd">
      <mp-searchbar bindselectresult="selectResult" search="{{search}}"></mp-searchbar>
  </view>

  <view class="page__bd page__bd_spacing">
    <view class="weui-flex">
      <view class="weui-flex__item">
        <view class="placeholder">weui</view>
      </view>
    </view>
    <view class="weui-flex">
      <view class="weui-flex__item">
        <view class="placeholder">weui</view>
      </view>
      <view class="weui-flex__item">
        <view class="placeholder">weui</view>
      </view>
    </view>
    <view class="weui-flex">
      <view class="weui-flex__item">
        <view class="placeholder">weui</view>
      </view>
      <view class="weui-flex__item">
        <view class="placeholder">weui</view>
      </view>
      <view class="weui-flex__item">
        <view class="placeholder">weui</view>
      </view>
    </view>
    <view class="weui-flex">
      <view class="weui-flex__item">
        <view class="placeholder">weui</view>
      </view>
      <view class="weui-flex__item">
        <view class="placeholder">weui</view>
      </view>
      <view class="weui-flex__item">
        <view class="placeholder">weui</view>
      </view>
      <view class="weui-flex__item">
        <view class="placeholder">weui</view>
      </view>
    </view>
    <view class="weui-flex">
      <view>
        <view class="placeholder">weui</view>
      </view>
      <view class="weui-flex__item">
        <view class="placeholder">weui</view>
      </view>
      <view>
        <view class="placeholder">weui</view>
      </view>
    </view>
  </view>

  <view class="btn-area">
    <view>
      <button class="mini-btn" type="primary" size="default" bindtap="getFile">选择文件</button>
      <button class="mini-btn" type="default" size="default" bindtap="getVideo">录音</button>
      <button class="mini-btn" type="warn" size="default" bindtap="mock">MockJS测试</button>
    </view>
  </view>

</view>


